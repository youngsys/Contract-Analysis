    <sql>UPDATE	"word"
SET		"word"."usages" = 
			CASE
				WHEN ISNUMERIC(LEFT("word",1))=1 OR LEN("word")&lt;4
				THEN 0
				ELSE ISNULL(sub."usages",0)
			END
,		"isValid" = 0
,		"startWord" = 0
FROM	"word"
LEFT	JOIN (	
			SELECT	"wordId"
			,		COUNT(*) AS "usages"
			FROM	(	SELECT	"sentenceId","wordId"
						,		COUNT(*) OVER (PARTITION BY "sentenceId", "wordId") AS "recs"
						FROM	"words"
						WHERE	"sentenceId" IN (
									SELECT	"sentenceId"
									FROM	"sentence"
									WHERE	"wordCount" &lt;= 100
								)
					) sub
			WHERE	"recs" = 1
			GROUP	BY "wordId"
		) sub
ON		sub."wordId" = word."wordId"</sql>
